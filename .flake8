[flake8]

max-line-length = 160
max-complexity = 10
inline-quotes = "

application-import-names = flake8-fill-one-lines
import-order-style = pycharm


exclude = .git, __pycache__, .idea, venv, tests/test_files

# ANN101 - type annotations for self
# N802 - function name should be lowercase
ignore = ANN101, N802